{
  "name": "lootandlevel",
  "version": "0.4.3",
  "description": "loot and level, a (massively) multiplayer online bullethell role playing browsergame",
  "scripts": {
    "start": "npx npm-run-all --parallel server client",
    "server": "npx nodemon ./src/server/index.js --ignore public",
    "client": "npx webpack-dev-server --config webpack.dev.js",
    "build": "npx webpack --config webpack.prod.js"
  },
  "author": "samsour",
  "license": "Apache 2.0",
  "dependencies": {
    "console-chat-client": "^0.1.2",
    "esm": "3.2.25",
    "nengi": "1.13.1",
    "pixi.js": "5.1.5"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.14.0",
    "nodemon": "1.19.3",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^13.2.0",
    "terser-webpack-plugin": "^2.3.4",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "3.8.2",
    "webpack-merge": "4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
