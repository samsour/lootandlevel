{
  "name": "lootandlevel",
  "version": "0.3.0",
  "description": "loot and level, a (massively) multiplayer online bullethell role playing browsergame",
  "scripts": {
    "start": "npx npm-run-all --parallel server client",
    "server": "npx nodemon ./src/server/index.js --ignore public",
    "client": "npx webpack-dev-server --config webpack.dev.js",
    "build": "npx webpack --config webpack.prod.js"
  },
  "author": "samsour",
  "license": "Apache 2.0",
  "dependencies": {
    "esm": "3.2.25",
    "nengi": "1.13.1",
    "pixi.js": "5.1.5"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "html-webpack-plugin": "^4.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "nodemon": "1.19.3",
    "npm-run-all": "4.1.5",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "^2.3.4",
    "webpack": "4.41.1",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.2",
    "webpack-merge": "4.2.2"
  }
}
